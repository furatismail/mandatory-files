  exampleObs() {
    const observable = new Observable((subscriber) => {
      subscriber.next(1);
      subscriber.next(2);
      subscriber.next(3);
      setTimeout(() => {
        subscriber.next(4);
      }, 1000);
      setTimeout(() => {
        subscriber.next(5);
        subscriber.complete();
      }, 2000);

    });
    return observable;
  }
