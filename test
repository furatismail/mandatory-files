    this.todosService.exampleObs().pipe(filter((val: any) => val > 2)).subscribe({
      next(x) {
        console.log('got value ' + x);
      },
      error(err) {
        console.error('something wrong occurred: ' + err);
      },
      complete() {
        console.log('done');
      },
    });
    
    
